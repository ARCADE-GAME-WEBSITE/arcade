[getUrlGame.length - 2] + \"/\" + getUrlGame[getUrlGame.length - 1]\r\n    \r\n    const [currentGame, setCurrentGame] = useState(null)\r\n    const gameLink = axios.defaults.baseURL + 'uploads/games'\r\n\r\n    useEffect(() => {\r\n        axios.get('/game/' + gameId)\r\n        .then((res)=>{\r\n            setCurrentGame(res.data)\r\n        })\r\n        .catch((err) => {\r\n            console.log(err);\r\n        })\r\n    },[])\r\n\r\n    return (currentGame) ? (\r\n        <div className=\"grid wide-1\">\r\n            <div className=\"row\">\r\n                <FriendList user={user}/>\r\n                <GameScreen gameLink={gameLink} currentGame={currentGame}/>\r\n            </div>\r\n            <div className=\"row\">\r\n                <GameControl/>\r\n            </div>\r\n            <div className=\"row\">\r\n                <GameInfo gameTitle={currentGame.Title}/>\r\n            </div>\r\n\r\n            <div className=\"row\">\r\n                <GameAbout gameDes={currentGame.Description} gameCreatedAt={currentGame.createdAt}/>\r\n                <GameDemo gameDemoUrl={currentGame.DemoUrl}/>\r\n            </div>\r\n            \r\n            <div className=\"row\">\r\n                {console.log(gameId)}\r\n                <GameComment user={user} gameId={gameId}/>\r\n                <GameTag/>\r\n                <GameDiff/>\r\n                <GameSame/>\r\n            </div>\r\n            \r\n        </div>\r\n    ): null;\r\n}\r\n\r\nexport default GameDetail","C:\\Users\\nghia\\Documents\\arcade\\front-end\\user_website\\src\\Pages\\UserAccount\\user-account.jsx",[],"C:\\Users\\nghia\\Documents\\arcade\\front-end\\user_website\\src\\Pages\\HomePage\\home-page.jsx",["169"],"C:\\Users\\nghia\\Documents\\arcade\\front-end\\user_website\\src\\Components\\Popup\\Form\\forgot-password.jsx",[],"C:\\Users\\nghia\\Documents\\arcade\\front-end\\user_website\\src\\Components\\Popup\\DialogBox\\dialog-box.jsx",[],"C:\\Users\\nghia\\Documents\\arca