)\r\n\r\n        console.log(gameImg);\r\n    return (\r\n        <div className=\"grid wide-1\">\r\n            <div className=\"row\">\r\n                <FriendList user={user}/>\r\n                <GameScreen gameLink={gameLink} gameSrc={gameSrc} gameId={gameId} gameImg={gameImg}/>\r\n            </div>\r\n            <div className=\"row\">\r\n                <GameControl/>\r\n            </div>\r\n            <div className=\"row\">\r\n                <GameInfo gameTitle={gameTitle}/>\r\n            </div>\r\n\r\n            <div className=\"row\">\r\n                <GameAbout gameDes={gameDes} gameCreatedAt={gameCreatedAt}/>\r\n                <GameDemo gameDemoUrl={gameDemoUrl}/>\r\n            </div>\r\n            \r\n            <div className=\"row\">\r\n                {console.log(gameId)}\r\n                <GameComment user={user} gameId={gameId}/>\r\n                <GameTag/>\r\n                <GameDiff/>\r\n                <GameSame/>\r\n            </div>\r\n            \r\n        </div>\r\n    );\r\n}\r\n\r\nexport default GameDetail","C:\\Users\\Administrator\\Desktop\\ArcadeMain\\front-end\\user_website\\src\\Pages\\UserAccount\\user-account.jsx",[],"C:\\Users\\Administrator\\Desktop\\ArcadeMain\\front-end\\user_website\\src\\Pages\\HomePage\\home-page.jsx",["168","169"],"C:\\Users\\Administrator\\Desktop\\ArcadeMain\\front-end\\user_website\\src\\Components\\Popup\\Form\\login.jsx",[],"C:\\Users\\Administrator\\Desktop\\ArcadeMain\\front-end\\user_website\\src\\Components\\Popup\\Form\\sign-up.jsx",["170","171"],"import React from \"react\";\r\nimport './form.css'\r\nimport useForm from './useForm'\r\nimport validator from './validator'\r\n\r\nexport const SignUp = ({showSignUp, setShowSignUp, setShowLogin, formRef, setDialogState}) => {\r\n    const {handleSubmit, handleClose, handleChange, values, errors} = useForm(\"SignUp\", formRef, setShowSignUp, validator, null, setDialogState)\r\n\r\n    return (showSignUp) ? (\r\n        <form onS