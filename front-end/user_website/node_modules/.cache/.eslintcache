\n                axios.post('user/login', values).then(\r\n                    res => {\r\n                        if (res.data.message === 'Authentication successful!'){\r\n                            localStorage.setItem('token', res.data.token);\r\n                            axios.defaults.headers.common['Authorization'] = res.data.token;\r\n                            setShowForm(false)\r\n                        }\r\n                    }\r\n                ).catch(\r\n                    err => {\r\n                        if (err.message === \"Request failed with status code 401\"){\r\n                            setErrors(validator(FormType, 'Wrong email or password!'))\r\n                        }\r\n                        else{\r\n                            setDialogState({\r\n                                title: \"Error!\",\r\n                                message: err.message,\r\n                                show: true\r\n                            })\r\n                        }\r\n                    }\r\n                )  \r\n            }\r\n            if (FormType === \"SignUp\"){\r\n                axios.post('user/sign-up', values).then(\r\n                    res => {\r\n                        setDialogState({\r\n                            title: \"Notify!\",\r\n                            message: res.data.message,\r\n                            show: true\r\n                        });\r\n                        setShowForm(false);\r\n                    }\r\n                ).catch(\r\n                    err => {\r\n                        setDialogState({\r\n                            title: \"Error!\",\r\n                            message: err.message,\r\n                            show: true\r\n                        })\r\n                    }\r\n                )\r\n            }\r\n    \r\n            if (FormType === \"ForgotPassword\"){\r\n                axios.post('user/forgot-password', values).then(\r\n       